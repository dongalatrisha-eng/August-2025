#linked is three types are single, double and circular linked list 

#single linked list 
class node:
    def __init__(self,val):
        self.data = val
        self.next = None
def SLL(arr):
    head = None
    for val in arr:
        if (head == None):
            head = node(val)
            temp = head
        else:
            temp.next = node(val)
            temp = temp.next
    return head 
arr = [2,3,4,5]
print(SLL(arr))
        
