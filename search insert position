class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        if not nums: return -1
        n = len(nums)
        if n == 1:
            return 0 if target <= nums[0] else 1
        
        l, r = 0, n - 1
        #print(l, l + (r - l) // 2, r)
        while l <= r:
            mid = l + (r - l) // 2
            if target > nums[mid]:
                l = mid + 1
            else:
                r = mid - 1
            #print(l, mid, r)
        return l
